{
  "transformations": [
    {
      "type": "rename_columns",
      "mapping": {
        "First Name": "GIVEN_NAME",
        "Last Name": "SURNAME",
        "Phone Number": "CONTACT_PHONE"
      }
    },
    {
      "type": "add_column",
      "name": "FULL_NAME",
      "expression": "GIVEN_NAME + ' ' + SURNAME"
    },
    {
      "type": "filter_rows",
      "condition": "Country in ['USA', 'Canada', 'UK', 'Germany', 'France']"
    },
    {
      "type": "apply_function",
      "column": "Email",
      "function": "lower"
    },
    {
      "type": "replace_values",
      "column": "Country",
      "mapping": {
        "USA": "United States",
        "UK": "United Kingdom"
      }
    },
    {
      "type": "sort",
      "columns": ["Country", "City", "GIVEN_NAME"],
      "ascending": true
    }
  ]
}